def write_coordinates(filename, l):
    with open(filename,"r") as f:
        for i in l:
            f.write(l[i][0],l[i][1],"\n")


def read_coordinates(filename):#Pas tr√®s beau mais fonctionne
    l = []
    with open(filename,"r") as f:
        flist = f.readlines()
        for i in flist:
            l.append(i, i+1)
            i+=1
    return l
